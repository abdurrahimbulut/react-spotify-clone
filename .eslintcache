     dispatch(changeSong(`${process.env.PUBLIC_URL}${path}`));\r\n    }\r\n\r\n    useEffect(() => {\r\n        audio.pause();\r\n        dispatch(pauseSong());\r\n        setAudio(new Audio(state.songPath));\r\n    },[state.songPath]);\r\n\r\n    useEffect(() => {\r\n        state.play ? audio.play() : audio.pause();\r\n    },[state.play]);\r\n    \r\n      \r\n    useEffect(() => {\r\n        audio.addEventListener('ended', () =>  dispatch(pauseSong()) );\r\n        return () => {\r\n            audio.removeEventListener('ended', () =>  dispatch(pauseSong()));\r\n        };\r\n    }, []);\r\n  \r\n    return [toggle, handleChangeSong];\r\n};\r\n\r\n\r\nconst PlayingBar = (props) => {\r\n    \r\n    const [toggle,handleChangeSong] = useAudio();\r\n\r\n\r\n  \r\n    return (\r\n        <div className={styles.PlayingBar}>\r\n            <button onClick={()=>toggle()} >Play/Pause</button>\r\n            <button onClick={()=>handleChangeSong(\"/songs/baris-manco-unutamadim.mp3\")} >test</button> \r\n\r\n        </div>\r\n    )\r\n}\r\nexport default PlayingBar;","C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\MainView\\MainView.jsx",["193"],"import React from 'react'\r\nimport Title from '../Typography/Title';\r\nimport styles from'./MainView.module.scss';\r\n\r\nconst MainView = (props) => {\r\n    return (\r\n        <div className={styles.MainView}>\r\n            {props.children}\r\n        </div>\r\n    )\r\n}\r\nexport default MainView;","C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Typography\\Title.jsx",[],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Icon\\Logo.jsx",[],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Buttons\\Button.jsx",["194"],"import React from 'react'\r\nimport {\r\n    Link\r\n  } from \"react-router-dom\";\r\nimport cn from 'classnames';\r\nimport styles from './Button.module.scss';\r\nimport { useEffect } from \"react\";\r\n\r\n\r\nconst LinkButton = ({href, children