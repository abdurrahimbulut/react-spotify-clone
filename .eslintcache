e from \"./Pages/CollectionPage\";\nimport SearchPage from \"./Pages/SearchPage\";\n\n\nfunction App() {\n  return (\n    <div className={styles.Main}>\n      <Router>\n        <TopBar />\n        <NavBar><h1>Navbar</h1></NavBar>\n        <PlayingBar><h1>Playing Bar</h1></PlayingBar>\n        <MainView>\n          <Switch>\n            <Route path={\"/\"} component={IndexPage} exact />\n            <Route path={\"/search\"} component={SearchPage} />\n            <Route path={\"/collection\"} component={CollectionPage}  />\n            <Route path={'/playlist/:id'} component={PlayListPage} />\n          </Switch>\n        </MainView>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n","C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\TopBar\\TopBar.jsx",[],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\NavBar\\NavBar.jsx",["195"],"import React from 'react'\r\nimport Button from '../Buttons/Button';\r\nimport Logo from '../Icon/Logo';\r\nimport styles from'./NavBar.module.scss';\r\nimport NavList from './NavList';\r\nimport PlayList from './PlayList';\r\n\r\nconst NavBar = (props) => {\r\n    return (\r\n        <div className={styles.NavBar}> \r\n            <div className={styles.NavHeader}>\r\n                <Logo width=\"131px\" ></Logo>\r\n            </div>\r\n            <NavList/>\r\n            <PlayList />\r\n        </div>\r\n    )\r\n}\r\nexport default NavBar;","C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\PlayingBar\\PlayingBar.jsx",["196","197","198","199"],"import React,{useEffect, useState} from 'react'\r\nimport styles from'./PlayingBar.module.scss';\r\nimport { useSelector,useDispatch } from \"react-redux\";\r\nimport { playSong,pauseSong,changeSong } from \"../../Redux/Actions/songActions\";\r\n\r\nconst useAudio = () => {\r\n    const dispatch = useDispatch();\r\n    const state = useSelector(state => state.songReducer);\r\n    const [audio,setAudio] = useState(new Audio(``));\r\n\r\n\r\n    \r\n    const toggle = () => {state.play ? dispatch(pauseSong()) : dispatch(playSong());};\r\n\r\n    //-\r\n    //her şarkı değiştiğinde oto play | not: burada bug var stateyi değiştirmedin\r\n    //audio.addEventListener('DOMAttrModified', audio.play(), true);\r\n    //-\r\n    useEffect(() => {\r\n        audio.play();\r\n        dispatch(playSong());\r\n    },[audio]);\r\n\r\n    const handleChangeSong=(path) => {\r\n        dispatch(changeSong(`${process.env.PUBLIC_URL}${path}`));\r\n    }\r\n\r\n    useEffect(() => {\r\n        audio.pause();\r\n        dispatch(pauseSong());\r\n        setAudio(new Audio(state.songPath));\r\n    },[state.songPath]);\r\n\r\n    useEffect(() => {\r\n        state.play ? audio.play() : audio.pause();\r\n    },[state.play]);\r\n    \r\n      \r\n    useEffect(() => {\r\n        audio.addEventListener('ended', () =>  dispatch(pauseSong()) );\r\n        return () => {\r\n            audio.removeEventListener('ended', () =>  dispatch(pauseSong()));\r\n        };\r\n    }, []);\r\n  \r\n    return [toggle, handleChangeSong];\r\n};\r\n\r\n\r\nconst PlayingBar = (props) => {\r\n    \r\n    const [toggle,handleChangeSong] = useAudio();\r\n\r\n\r\n  \r\n    return (\r\n        <div className={styles.PlayingBar}>\r\n            <button onClick={()=>toggle()} >Play/Pause</button>\r\n            <button onClick={()=>handleChangeSong(\"/songs/baris-manco-unutamadim.mp3\")} >test</button> \r\n\r\n        </div>\r\n    )\r\n}\r\nexport default PlayingBar;","C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\MainView\\MainView.jsx",["200"],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Typography\\Title.jsx",[],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Icon\\Logo.jsx",[],"C:\\xampp\\htdocs\\react\\react-spotify-clone\\src\\Components\\Buttons\\Button.jsx",["201"],"import React from 'react'\r\nimport {\r\n    Link\r\n  } from \"react-router-dom\";\r\nimport cn from 'classnames';\r\ni